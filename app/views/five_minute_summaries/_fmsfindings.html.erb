<!-- BEGIN FIVE MINUTE SUMMARY SECTION OVERALL ASSESSMENT-->
<a name="overallassessment"></a>
<div class="sectionWrapper" id="WPoverallassessment">
<div class="row contentbg">

  <div class="small-12 medium-3 columns">
    <div class="RAfull">
      
      
      <h4 data-magellan-destination="overallassessment" class="text-right">OVERALL ASSESSMENT</h4> 
    </div>
  </div>

  <div class="small-12 medium-9 columns">
    <div class="RAfull">

					<% unless thissection.content_blocks.empty? %>

						<% thissection.block_order.each_with_index do |bid, index| %>
							<% thisblock = thissection.content_blocks.find(bid) # iterate through content blocks in order%>
							
								<%= thisblock.content.html_safe %>
							
						<% end %>

					<% end %>
  	</div>
	</div>
</div>
</div>
<!-- END FIVE MINUTE SUMMARY SECTION OVERALL ASSESSMENT-->